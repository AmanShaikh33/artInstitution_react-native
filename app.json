{
  "expo": {
    "name": "art_institution",
    "slug": "art_institution",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/music.png",
    "scheme": "artinstitution",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/music.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.amanshaikh33.art_institution"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/music.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/music.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1f28faee-c8ef-49ef-b3f1-d3c767af5d05"
      }
    }
  }
}
